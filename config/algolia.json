{
  "index_name": "prod_docs",
  "start_urls": ["https://buildkite.com/docs"],
  "stop_urls": ["agent/v2"],
  "selectors": {
    "lvl0": {
      "selector": ".SiteHeader__nav .Nav__link.Nav__link--level1.Nav__link--current",
      "global": true
    },
    "lvl1": {
      "selector": ".SiteHeader__nav .Nav__link.Nav__link--level2.Nav__link--parent.Nav__toggle--on",
      "global": true
    },
    "lvl2": ".Article h1",
    "lvl3": ".Article h2",
    "lvl4": ".Article h3",
    "lvl5": ".Article h4, .Article th",
    "text": ".Article p, .Article li, .Article td"
  },
  "selectors_exclude": [
    "[data-algolia-exclude]",
    ".Toc",
    ".Footer",
    ".Page__sidebar",
    ".Nav__link--level2:not(.Nav__link--parent.Nav__toggle--on)",
    ".Nav__section--level3"
  ],
  "custom_settings": {
    "synonyms": [["Pipeline.yml", "Pipeline.yaml", "config.yml", "config.yaml"]]
  }
}
